@Article{IshimaTorchia05,
  Author         = {Ishima, R. and Torchia, D. A.},
  Title          = {Error estimation and global fitting in
                   transverse-relaxation dispersion experiments to
                   determine chemical-exchange parameters.},
  Journal        = jbnmr,
  Volume         = {32},
  Number         = {1},
  Pages          = {41-54},
