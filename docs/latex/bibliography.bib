  medline-dcom   = {19901205},
  medline-edat   = {1990/08/14},
  medline-ein    = {Biochemistry 1991 Jan 8;30(1):312},
  medline-fau    = {Clore, G M ; Driscoll, P C ; Wingfield, P T ;
                   Gronenborn, A M},
  medline-is     = {0006-2960},
  medline-jid    = {0370623},
